{
    "go.testOnSave": true,
    "go.lintOnSave": "package",
    "go.formatTool": "gofumports",
    "go.testFlags": [
        "-v"
    ],
    "go.autocompleteUnimportedPackages": true,
    "[go]":{
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    }
}